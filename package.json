  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix",
    "electron:dev": "tsc -p electron/tsconfig.json && concurrently \"npm run dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "electron:dev:win": "tsc -p electron/tsconfig.json && concurrently \"npm run dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "electron:start": "tsc -p electron/tsconfig.json && electron .",
    "electron:build": "npm run build && tsc -p electron/tsconfig.json && electron-builder",
    "electron:build:win": "npm run build && tsc -p electron/tsconfig.json && electron-builder --win",
    "electron:build:mac": "npm run build && tsc -p electron/tsconfig.json && electron-builder --mac",
    "electron:build:linux": "npm run build && tsc -p electron/tsconfig.json && electron-builder --linux",
    "electron:publish": "npm run build && tsc -p electron/tsconfig.json && electron-builder --publish always",
    "electron:publish:win": "npm run build && tsc -p electron/tsconfig.json && electron-builder --win --publish always",
    "electron:publish:mac": "npm run build && tsc -p electron/tsconfig.json && electron-builder --mac --publish always",
    "electron:publish:linux": "npm run build && tsc -p electron/tsconfig.json && electron-builder --linux --publish always"
  },
